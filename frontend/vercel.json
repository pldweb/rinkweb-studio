{
  "version": 2,
  "name": "rinkweb-studio-frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "framework": "vite",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://admin.rinkwebstudio.com/api/$1",
      "headers": {
        "Access-Control-Allow-Origin": "https://rinkwebstudio.com",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization, X-Requested-With, Accept, Origin, X-CSRF-TOKEN, X-XSRF-TOKEN",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/sanctum/(.*)",
      "dest": "https://admin.rinkwebstudio.com/sanctum/$1",
      "headers": {
        "Access-Control-Allow-Origin": "https://rinkwebstudio.com",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization, X-Requested-With, Accept, Origin, X-CSRF-TOKEN, X-XSRF-TOKEN",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/login",
      "dest": "https://admin.rinkwebstudio.com/login",
      "status": 302
    },
    {
      "src": "/admin(.*)",
      "dest": "https://admin.rinkwebstudio.com$1",
      "status": 302
    },
    {
      "src": "/dashboard(.*)",
      "dest": "https://admin.rinkwebstudio.com/dashboard$1",
      "status": 302
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|webp|mp4|webm|ogg|mp3|wav|flac|aac))",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://rinkwebstudio.com"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization, X-Requested-With, Accept, Origin, X-CSRF-TOKEN, X-XSRF-TOKEN"
        },
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        }
      ]
    }
  ],
  "env": {
    "VITE_APP_NAME": "RinkWeb Studio",
    "VITE_APP_ENV": "production",
    "VITE_APP_URL": "https://rinkwebstudio.com",
    "VITE_API_BASE_URL": "https://admin.rinkwebstudio.com",
    "VITE_API_URL": "https://admin.rinkwebstudio.com/api",
    "VITE_BACKEND_URL": "https://admin.rinkwebstudio.com"
  },
  "functions": {
    "app/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "regions": ["sin1"],
  "github": {
    "enabled": true,
    "autoAlias": false
  },
  "redirects": [
    {
      "source": "/www/(.*)",
      "destination": "/$1",
      "permanent": true
    },
    {
      "source": "/home",
      "destination": "/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/api/sitemap"
    },
    {
      "source": "/robots.txt",
      "destination": "/api/robots"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}